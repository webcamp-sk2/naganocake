<div class="container">
   <div class="row">
  <div class='col-10 offset-1'>
   <h2>会員一覧</h2>
 </div>

 <div class="col-xs-10 member-index">
  <table class = "list">
   <thead>
     <tr>会員ID</tr>
     <tr>氏名</tr>
     <tr>メールアドレス</tr>
     <tr>ステータス</tr>
   </thead>
   <tbody>
     <% @publics.each do |public| %>
    <tr>
      <td><%= public.id %></td>
      <td><%= public.first_name %></td>
      <td><%= public.email %></td>
      <td><%= public.is_deleted %></td>
    </tr>
    <% end %>
   </tbody>
  </table>
   <%= paginate @publics %>
 </div>
 </div>
</div>